

<div class="card">
    <div class="card-header">
        <h2 class="mb-0">CoinOne</h2>
    </div>
    <div class="card-body">
        
    </div>
</div>


@section Scripts{


    <script>

        $(function(){
            GetOrderBook();
            GetTick();
        })

        function GetOrderBook(){
            $.ajax({
                method : 'Get',
                url : `/Coinone/GetOrderBook`,
                success : (res) => {
                    console.log(res);
                },
                error : (error) => {
                    console.error(error);
                }
            })
        }


        function GetTick(){
            $.ajax({
                method : 'Get',
                url : `/Coinone/GetTick`,
                success : (res) => {
                    console.log(res);
                },
                error : (error) => {
                    console.error(error);
                }
            })
        }
    </script>
}